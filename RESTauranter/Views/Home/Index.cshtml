@model review

<html>
    <head>
        <title></title>
    </head>
    <body>
        <h1>Write a New Review</h1>
        <form action="submit" method="POST">
            <div class="main">

                <p><label asp-for="reviewername">Reviewer Name</label></p>
                <p><input asp-for="reviewername"></p>
                <p><label asp-for="restaurantname">Restaurant Name</label></p>
                <p><input asp-for="restaurantname"></p>
                <p><label asp-for="reviews">Review</label></p>
                <p><textarea asp-for="reviews" cols="30" rows="10"></textarea></p>
                <p><label asp-for="star">Stars:</label>
                    <select asp-for="star">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                    </select></p>
                <p><label asp-for="date">Date of Visit:</label></p>
                <p><input asp-for="date"></p>
                <P><button type="submit" >Submit</button></P>
            
            </div>
        </form>
    </body>
</html>
  
  
    
  
  
  
          
  